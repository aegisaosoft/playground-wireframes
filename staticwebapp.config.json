{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/avatars/*",
      "/favicon.ico",
      "/robots.txt",
      "/api/*"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  }
}